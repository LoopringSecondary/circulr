  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/dist/preprocessor.js"
    },
    "testRegex": "tests/__tests__/.*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "tests/__tests__/watch.spec.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "coverageDirectory": "test_coverage_dir",
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
